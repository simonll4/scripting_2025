{
  "name": "@tpfinal/session-store",
  "version": "1.0.0",
  "description": "Session Store API - gestiona sesiones y detecciones",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "db:migrate": "node -r ts-node/register src/db/migrate.ts",
    "db:seed": "node -r ts-node/register src/db/seed.ts",
    "migrate": "npm run db:migrate",
    "seed": "npm run db:seed"
  },
  "dependencies": {
    "@tpfinal/shared": "file:../../shared",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.10.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/pg": "^8.10.2",
    "@types/cors": "^2.8.14",
    "@types/compression": "^1.7.3",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.12"
  }
}